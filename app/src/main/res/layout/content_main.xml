<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.ania.krs20.MainActivity"
    tools:showIn="@layout/activity_main"
    android:orientation="vertical"
    android:weightSum="1"
    android:background="#ffffff">


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="191dp"
        android:id="@+id/imageView3"
        android:src="@drawable/logo"
        android:layout_weight="0.41" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Wyszukiwarka"
        android:id="@+id/button2"
        android:layout_gravity="center_horizontal"
        android:onClick="wysz"/>

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Akcje i udziały"
        android:id="@+id/button4"
        android:layout_gravity="center_horizontal"
        android:onClick="akcje"/>

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Wspólnicy i nadzorcy"
        android:id="@+id/button16"
        android:layout_gravity="center_horizontal"
        android:onClick="ludzie"/>

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Podział forma prawana"
        android:id="@+id/button17"
        android:layout_gravity="center_horizontal"
        android:onClick="podzialF"/>

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Podział na województwa"
        android:id="@+id/button18"
        android:layout_gravity="center_horizontal"
        android:onClick="podzialW"/>

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Autorzy"
        android:id="@+id/button19"
        android:layout_gravity="center_horizontal"
        android:onClick="autorzy"/>

</LinearLayout>
